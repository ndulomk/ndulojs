{
  "extends": "./tsconfig.json",
  "include": [
    "src/**/*",
    "test/**/*",
    "vitest.config.ts",
    "build.ts",
    ".eslintrc.cjs"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noEmit": true,
    "rootDir": "."
  }
}